#########################################################
layer {
  name: "concat_limb"
  type: "Concat"
  bottom: "label"
  bottom: "limb_label"
  top: "concat_limb"
  concat_param {
    concat_dim: 1
  }
}
#########################################################
layer {
  name: "loss_fusion"
  type: "EuclideanLossHeatmap"
  bottom: "conv6_fusion"
  bottom: "concat_limb"
  bottom: "data"  
  top: "loss_fusion"
  visualise: false
  loss_weight: 1
}
